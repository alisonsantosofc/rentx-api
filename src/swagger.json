{
  "openapi": "3.0.0",
  "info": {
    "title": "Auto Renter API Docs",
    "description": "This is an API to rent cars",
    "version": "1.0.0",
    "contact": {
      "email": "alison.frontend.developer@gmail.com"
    }
  },
  "paths": {
    "/categories": {
      "post": {
        "tags": ["Categories"],
        "summary": "Create Category",
        "description": "Create a new category.",
        "security": [{"bearerAuth": []}],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "Category Name Sample",
                  "description": "Category description sample."
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created."
          },
          "400": {
            "description": "Category already exists."
          }
        }
      },
      "get": {
        "tags": ["Categories"],
        "summary": "List Categories",
        "description": "List all categories.",
        "responses": {
          "200": {
            "description": "Successfully listed.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },

    "/categories/import": {
      "post": {
        "tags": ["Categories"],
        "summary": "Upload Categories",
        "description": "Upload categories from a csv file.",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "responses": {
            "201": {
              "description": "Created."
            }
          }
        }
      }
    },

    "/specifications": {
      "post": {
        "tags": ["Specifications"],
        "summary": "Create Specification",
        "description": "Create a new specification.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Specification"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created."
          },
          "400": {
            "description": "Specification already exists."
          }
        }
      }
    },

    "/sessions": {
      "post": {
        "tags": ["Sessions"],
        "summary": "User Authentication",
        "description": "Authentication an user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Email or password incorrect."
          }
        }
      }
    }
  },
  "definitions": {
    "Specification": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}